{
  "expo": {
    "name": "passes",
    "slug": "passes",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "passes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "backgroundColor": "#000000",
      "edgeToEdgeEnabled": true
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#00000000"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
